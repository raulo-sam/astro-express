{
  "extends": "astro/tsconfigs/strict",
  // "include": [
  //   "./src/**/*.ts",
  //   "./src/**/*.d.ts",
  //   "./src/**/*.astro",
  // ],
  // "exclude": ["node_modules", "build"],
  "compilerOptions": {
    "types": ["node", "express"],
    "noImplicitAny": false,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "strictNullChecks": false,
    "isolatedModules": false,
    "baseUrl": ".",
    "paths": {
      "@UI/*": ["src/UI/*"]
    }
    
  }
}