{
  "name": "@example/basics",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "node server-ssr.js",
    "dev:ssg": "nodemon server-ssg.js",
    "dev:entry": "nodemon ./dist/server/entry.mjs",
    "build:client": "vite --config vite.config.client.js build --outDir dist/client",
    "build:server": "vite --config vite.config.server.js build --outDir dist/server --ssr src/entry-server.jsx",
    "build": "npm run build:client && npm run build:server && node prerender"

  },
  "dependencies": {
    "@vitejs/plugin-react": "^4.2.1",
    "@astrojs/markdown-remark": "^5.1.0",
    "express": "^4.19.2",
    "node-yaml": "^4.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rollup": "^4.17.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.10"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/react-dom": "^18.3.0"
  },
  "engines": {
    "node": ">=16"
  }
}
